int pattern() {
  var array = [1, 2, 5, 1];


if (array[0]<=9 && array[1]<=9 && array[2]<=9 && array[3]<=9  ) {
  for (int i = 0; i < array.length; i++) {
    for (int j = 0; j < array.length; j++) {
      for (int k = 0; k < array.length; k++) {
        if (j != i) {
          print('${array[i]} ${array[j]} ${array[k]}');
        }
      }
    }
  }
}
  else{
    print ('index value is greater than 9');
  }
  return 0;
}

int main() {
  pattern();
  return 0;
}
