//Without using function
int main() {
  


  int num=5;
  
 List  array=[];
  
  for(int i=1;i<=num;i++){
  array.add(i);  
  }
  for(int i=num-1;i>=1;i--)
  {
  array.add(i);
  }
  print(array);

return 0;
}
//With using Functions
dynamic palindromic(int num){
  

List array=[];
for(int i=1;i<=num;i++){
array.add(i);  
}
for(int i=num-1;i>=1;i--)
{
  array.add(i);
}

return   array ;

}



int main() {
  int num=5;

  

print(palindromic(num));
return 0;
}
